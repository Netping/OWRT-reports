config globals 'globals'
	list method 'email.Почта'
	list method 'snmptrap.Отправить snmp сообщение'
	list period 'hourly.Каждый час'
	list period 'daily.Каждый день'
	list period 'weekly.Каждую неделю'
	list period 'monthly.Каждый месяц'

config report 'prototype'
	option name 'Default report'
	option description 'Description text here'
	option state '0'
	option callbacks '[ { "module" : "owrt_module", "method" : "owrt_method", "input_parameters" : { "parameter" : "value" }, "output_parameters" : { "parameter" : "" } } ]'
	option method 'empty'
	option schedule 'none'
	option text 'Default text'

